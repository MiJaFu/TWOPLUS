}

.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 200rpx 0;
    box-sizing: border-box;
}

b {
    font-weight: bold;
}

view, span, text, b {
    color: #101010;
    font-family: Arial, Helvetica, sans-serif;
}
.bold{
    font-weight: bold;
}
.hide{
    display: none !important;
}
.block{
    display: block!important;
}

